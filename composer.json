{
  "name": "rammehraz/bot",
  "description": "A simple Telegram bot in PHP",
  "type": "project",
  "require": {
    "php": "^8.0",
    "vlucas/phpdotenv": "^5.4",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^7.4",
    "react/http": "^1.6",
    "react/event-loop": "^1.3",
    "predis/predis": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Bot\\": "classes/",
      "Config\\": "config/",
      "Payment\\": "payment/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}